{
  "name": "Coral Reef",
  "type": "light",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "#EFA536"
      }
    },
    {
      "name": "number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#3675EF"
      }
    },
    {
      "name": "variable definition",
      "scope": "variable, meta.definition.variable",
      "settings": {
        "foreground": "#21AD7F"
      }
    },
    {
      "name": "function",
      "scope": "entity.name.function, meta.function-call, entity.name.type",
      "settings": {
        "foreground": "#323232"
      }
    },
    {
      "name": "comment",
      "scope": "comment",
      "settings": {
        "foreground": "#B9B9DA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#C73161"
      }
    },
    {
      "name": "children",
      "scope": "meta.jsx.children",
      "settings": {
        "foreground": "#323232"
      }
    },
    {
      "name": "type",
      "scope": "meta.type, entity.name.type",
      "settings": {
        "foreground": "#767676"
      }
    },
    {
      "name": "tag",
      "scope": "meta.tag",
      "settings": {
        "foreground": "#21AD7F"
      }
    },
    {
      "name": "tag attributes",
      "scope": "meta.tag.attributes, entity.other.attribute-name.html",
      "settings": {
        "foreground": "#8147DF"
      }
    },
    {
      "name": "tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#323232"
      }
    },
    {
      "name": "keyword control",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#C73161"
      }
    },
    {
      "name": "keywords import / from",
      "scope": "keyword.control.import, keyword.control.from",
      "settings": {
        "foreground": "#3675EF"
      }
    },
    {
      "name": "import variable",
      "scope": "variable.other.readwrite.alias",
      "settings": {
        "foreground": "#323232"
      }
    },
    {
      "name": "keywords assignment",
      "scope": "keyword.operator.assignment, punctuation.separator.key-value, meta.tag.svg",
      "settings": {
        "foreground": "#323232"
      }
    },
    {
      "name": "type alias / interface",
      "scope": "entity.name.type.alias, entity.name.type.interface",
      "settings": {
        "foreground": "#21AD7F"
      }
    },
    {
      "name": "docker",
      "scope": "keyword.other.special-method",
      "settings": {
        "foreground": "#21AD7F"
      }
    },
    {
      "name": "json & yaml property",
      "scope": "support.type.property-name.json, entity.name.tag.yaml",
      "settings": {
        "foreground": "#323232"
      }
    },
    {
      "name": "markdown link title",
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#EFA536"
      }
    },
    {
      "name": "markdown link description",
      "scope": "string.other.link.description.markdown",
      "settings": {
        "foreground": "#21AD7F"
      }
    },
    {
      "name": "markdown - link",
      "scope": "markup.underline.link.image.markdown, markup.underline.link.markdown",
      "settings": {
        "foreground": "#8147DF"
      }
    },
    {
      "name": "css - tag",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#8147DF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "css - class / id selector",
      "scope": "entity.other.attribute-name.class.css, entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#EFA536"
      }
    },
    {
      "name": "css - pseudo class & elements",
      "scope": "entity.other.attribute-name.pseudo-class, entity.other.attribute-name.pseudo-element",
      "settings": {
        "foreground": "#C73161"
      }
    },
    {
      "name": "css - property name",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#21AD7F"
      }
    },
    {
      "name": "css - numeric property",
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#323232"
      }
    },
    {
      "name": "css - keyword important",
      "scope": "keyword.other.important.css",
      "settings": {
        "foreground": "#C73161"
      }
    },
    {
      "name": "svg - tag",
      "scope": "entity.name.tag.localname.svg",
      "settings": {
        "foreground": "#21AD7F"
      }
    },
    {
      "name": "svg - tag attributes separator",
      "scope": "entity.other.attribute-name.localname.svg",
      "settings": {
        "foreground": "#8147DF"
      }
    }
  ],
